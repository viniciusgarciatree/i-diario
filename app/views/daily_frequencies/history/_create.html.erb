<table class="table table-striped records">
  <thead>
    <tr>
      <th>Campo</th>
      <th>Valor</th>
    </tr>
  </thead>
  <% record.audited_changes.each do |key, values| %>
    <% next if key == 'sequence' || key == 'discarded_at' %>
    <tbody>
      <tr>
        <td>
          <%= record.human_attribute_name key %>
        </td>
        <td><%= raw record.parse(key.to_sym, values, :first).to_s %></td>
      </tr>
    <% end %>
  </tbody>
</table>
